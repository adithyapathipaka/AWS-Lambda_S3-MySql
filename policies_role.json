{  "Version": "2012-10-17",  "Statement": [    {      "Sid": "s3permissions2020",      "Effect": "Allow",      "Action": [		  "s3:ListStorageLensConfigurations",                "s3:PutObject",                "s3:GetObject",                "s3:ListBucketMultipartUploads",                "s3:ListAllMyBuckets",                "s3:ListJobs",                "s3:ListBucketVersions",                "s3:ListBucket",                "s3:DeleteObject",                "s3:ListMultipartUploadParts"                     ],      "Resource": "*"    },    {      "Sid": "rdspermissions2020",      "Effect": "Allow",      "Action": [        "rds-db:connect"      ],      "Resource": "arn:aws:rds-db:<aws-region>:<aws-account-id>:dbuser:<db-resource-id>/*"    },    {      "Sid": "ssmpermissions2020",      "Effect": "Allow",      "Action": [        "ssm:Describe*",        "ssm:Get*",        "ssm:List*"      ],      "Resource": "*"    },    {      "Sid": "cloudwatchpermissions2020",      "Effect": "Allow",      "Action": [        "logs:CreateLogGroup",        "logs:CreateLogStream",        "logs:PutLogEvents"      ],      "Resource": "*"    },	  {	   "Sid": "ec2services",            "Effect": "Allow",            "Action": [                "ec2:CreateNetworkInterface",                "ec2:DescribeNetworkInterfaces",                "ec2:DeleteNetworkInterface",                "ec2:AssignPrivateIpAddresses",                "ec2:UnassignPrivateIpAddresses"            ],            "Resource": "*"        }  ]}